//A Java example about data structures
import java.util.ArrayList;

public class Java5 {

	public static void main(String[] args) {
		
		int[] var = {1,2,3};
		System.out.println(var[2]);
		
		int[] array = new int[10];
		for(int i = 0; i < 10; i++) {
			array[i] = i;
		}
		System.out.print(array[5]);

	}

}
